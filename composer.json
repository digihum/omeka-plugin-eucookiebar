{
  "name": "digihum/eucookiebar",
  "type": "omeka-plugin",
  "description": "The plugin is a wrapper for the GDPR/CCPA compliant Cookie Consent JavaScript library. It provides a simple way to add a customisable cookie notice to your Omeka site."
  "authors": [{
      "name": "Steve Ranford",
      "email": "steve.ranford@warwick.ac.uk",
      "role": "Research Software Engineer"
    }],
  "license": "GPLv3",
  "readme": "README.md",
  "version": "2.0.0-beta.2",
  "require": {
    "omeka/omeka": ">=2.0"
  },
  "extra": {
    "installer-name": "EUCookieBar"
  }
}

link = "https://github.com/digihum/omeka-plugin-eucookiebar"
support_link = "https://github.com/digihum/omeka-plugin-eucookiebar/issues"
omeka_minimum_version = "2.0"
omeka_target_version = "2.5"
required_plugins = ""
